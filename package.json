{
	"name": "svelt-version",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=18.x"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:analyze": "vite build --mode analyze",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync --tsconfig ./tsconfig.json --watch",
		"check:svelte": "svelte-check",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"validate": "npx run-s validate:quality lint check",
		"validate:quality": "node scripts/validate-code-quality.js",
		"format": "prettier --write \"src/**/*.{js,ts,svelte,css,html,json}\"",
		"format:check": "prettier --check \"src/**/*.{js,ts,svelte,css,html,json}\"",
		"build:release": "node scripts/create-release.js",
		"release": "npx run-s validate build:release",
		"test": "node node_modules/vitest/vitest.mjs run",
		"test:unit": "node node_modules/vitest/vitest.mjs watch",
		"test:coverage": "node node_modules/vitest/vitest.mjs run --coverage",
		"optimize": "npx run-s validate test build"
	},
	"devDependencies": {
		"@babel/core": "^7.27.4",
		"@babel/types": "^7.27.3",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.28.0",
		"@iconify/json": "^2.2.284",
		"@sveltejs/adapter-netlify": "^5.0.2",
		"@sveltejs/kit": "^2.21.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^22.15.29",
		"@types/uuid": "^10.0.0",
		"@vite-pwa/sveltekit": "^0.6.6",
		"autoprefixer": "^10.4.21",
		"daisyui": "^4.12.24",
		"dotenv": "^16.5.0",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.7.0",
		"glob": "^10.3.10",
		"globals": "^16.2.0",
		"jiti": "^2.4.2",
		"jsdom": "^26.1.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.5.4",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"rollup": "^4.41.1",
		"rollup-plugin-visualizer": "^6.0.1",
		"svelte": "^5.33.11",
		"svelte-check": "^4.2.1",
		"svelte-eslint-parser": "^1.2.0",
		"tailwindcss": "^3.4.17",
		"terser": "^5.40.0",
		"tsx": "^4.7.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.0",
		"unplugin-icons": "^0.21.0",
		"vite": "^6.3.5",
		"vite-imagetools": "^7.0.4",
		"vite-plugin-checker": "^0.9.3",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-pwa": "^0.21.2",
		"vitest": "^3.1.4"
	},
	"dependencies": {
		"@fontsource/inter": "^5.2.5",
		"@giphy/js-fetch-api": "^5.6.0",
		"@jridgewell/sourcemap-codec": "^1.5.0",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.49.4",
		"caniuse-lite": "1.0.30001720",
		"cookie": "^1.0.2",
		"date-fns": "^4.1.0",
		"handsontable": "^15.3.0",
		"idb": "^8.0.3",
		"uuid": "^11.1.0",
		"web-vitals": "^5.0.2",
		"zod": "^3.25.46"
	},
	"packageManager": "npm@10.2.4",
	"overrides": {
		"glob": "^10.3.10",
		"sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0"
	}
}
