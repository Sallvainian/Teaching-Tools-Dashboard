{
	"name": "svelt-version",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=22.0.0",
		"npm": ">=11.0.0"
	},
	"scripts": {
		"dev": "npx vite dev",
		"build": "npx vite build",
		"build:analyze": "npx vite build --mode analyze",
		"preview": "npx vite preview",
		"prepare": "npx svelte-kit sync",
		"check": "npx svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "npx svelte-kit sync --tsconfig ./tsconfig.json --watch",
		"check:svelte": "npx svelte-check",
		"lint": "npx eslint .",
		"lint:fix": "npx eslint --fix .",
		"validate": "npx run-s validate:quality lint check",
		"validate:quality": "node scripts/validate-code-quality.js",
		"format": "npx prettier --write \"src/**/*.{js,ts,svelte,css,html,json}\"",
		"format:check": "npx prettier --check \"src/**/*.{js,ts,svelte,css,html,json}\"",
		"build:release": "node scripts/create-release.js",
		"release": "npx run-s validate build:release",
		"test": "npx vitest run",
		"test:unit": "npx vitest watch",
		"test:coverage": "npx vitest run --coverage",
		"optimize": "npx run-s validate test build"
	},
	"devDependencies": {
		"@babel/core": "^7.27.4",
		"@babel/types": "^7.27.6",
		"@eslint/compat": "^1.3.0",
		"@eslint/js": "^9.29.0",
		"@iconify/json": "^2.2.351",
		"@rollup/rollup-linux-x64-gnu": "^4.44.0",
		"@stylistic/eslint-plugin": "^4.4.1",
		"@sveltejs/adapter-netlify": "^5.0.2",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.0.3",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.21",
		"dotenv": "^16.5.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-import-x": "^4.15.2",
		"eslint-plugin-svelte": "^3.9.3",
		"eslint-plugin-unicorn": "^59.0.1",
		"glob": "^10.3.10",
		"globals": "^16.2.0",
		"jiti": "^2.4.2",
		"jsdom": "^26.1.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.5.6",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"rollup": "^4.44.0",
		"rollup-plugin-visualizer": "^6.0.3",
		"svelte": "^5.34.7",
		"svelte-check": "^4.2.2",
		"svelte-eslint-parser": "^1.2.0",
		"tailwindcss": "^3.4.17",
		"terser": "^5.43.1",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.1",
		"unplugin-icons": "^22.1.0",
		"vite": "^6.3.5",
		"vite-plugin-checker": "^0.9.3",
		"vite-plugin-compression": "^0.5.1",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@fontsource/inter": "^5.2.6",
		"@giphy/js-fetch-api": "^5.6.0",
		"@jridgewell/sourcemap-codec": "^1.5.0",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.50.0",
		"caniuse-lite": "1.0.30001724",
		"cookie": "^1.0.2",
		"date-fns": "^4.1.0",
		"idb": "^8.0.3",
		"pdfjs-dist": "^5.3.31",
		"uuid": "^11.1.0",
		"web-vitals": "^5.0.3",
		"zod": "^3.25.67"
	},
	"packageManager": "npm@10.9.2",
	"overrides": {
		"glob": "^10.3.10",
		"sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0",
		"@rollup/rollup-win32-x64-msvc": "npm:@rollup/rollup-linux-x64-gnu@4.44.0"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.44.0"
	}
}
